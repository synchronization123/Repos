        </div>
    </div>
    <script>
        function toggleSubmenu(event, menuId) {
            event.preventDefault();
            let submenu = document.getElementById(menuId);
            if (submenu.style.display === 'block') {
                submenu.style.display = 'none';
            } else {
                submenu.style.display = 'block';
            }
        }
    </script>
</body>
